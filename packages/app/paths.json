{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/frontend/*"],
      "src/*": ["src/*"],
      "root/*": ["./*"],
      "@hooks/*": ["src/frontend/hooks/*"],
      "@hooks": ["src/frontend/hooks/index.ts"],
      "@parser/*": ["src/frontend/parser/*"],
      "@parser": ["src/frontend/parser/index.ts"],
      "@components/*": ["src/frontend/components/*"],
      "@components": ["src/frontend/components/index.ts"],
      "@icons/*": ["src/frontend/icons/*"],
      "@icons": ["src/frontend/icons/index.ts"],
      "@containers/*": ["src/frontend/containers/*"],
      "@containers": ["src/frontend/containers/index.ts"],
      "@store/*": ["src/frontend/store/*"],
      "@store": ["src/frontend/store/index.ts"],
      "@styles/*": ["src/frontend/styles/*"],
      "@styles": ["src/frontend/styles/index.ts"],
      "@static/*": ["src/frontend/static/*"],
      "@static": ["src/frontend/static/index.ts"],
      "@utils/*": ["src/frontend/utils/*"],
      "@utils": ["src/frontend/utils/index.ts"],
      "@modules/*": ["src/frontend/modules/*"],
      "@modules": ["src/frontend/modules/index.ts"],
      "@views/*": ["src/frontend/views/*"],
      "@views": ["src/frontend/views/index.ts"],
      "@pages/*": ["src/frontend/pages/*"],
      "@pages": ["src/frontend/pages/index.ts"],
      "@layouts/*": ["src/frontend/layouts/*"],
      "@layouts": ["src/frontend/layouts/index.ts"],
      "@stitched/*": ["src/frontend/stitched/*"],
      "@stitched": ["src/frontend/stitched/index.ts"],
      "@middlewares/*": ["src/frontend/middlewares/*"],
      "@middlewares": ["src/frontend/middlewares/index.ts"]
    }
  }
}
